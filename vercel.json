{
  "version": 2,
  "framework": null,
  "buildCommand": "echo 'No build needed'",
  "devCommand": "npx serve . -p 3000",
  "installCommand": "echo 'No dependencies'",
  "outputDirectory": ".",
  "routes": [
    {
      "src": "/api/(.*)",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
